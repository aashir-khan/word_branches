{
  "name": "word-branches",
  "license": "MIT",
  "main": "./dist/dmain.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "postinstall": "tsc",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "start:dev": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:dev-watch": "nodemon",
    "build": "tsc"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "eslint": "^7.22.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "@types/jest": "^26.0.21",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.35",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "http-proxy-middleware": "^1.3.0",
    "morgan": "^1.10.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start:dev",
    "ext": "ts"
  }
}
